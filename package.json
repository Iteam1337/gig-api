{
  "name": "gig-api",
  "version": "1.0.0",
  "description": "Gig API",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run migrate && npm run start:server",
    "start:server": "node ./lib/index.js",
    "migrate": "bin/migrate up",
    "migrate:down": "bin/migrate down",
    "migrate:create": "bin/migrate create",
    "migrate:integration": "bin/migrate integration",
    "dev": "npm run migrate && nodemon",
    "test": "mocha test/{unit/**,unit}/*.spec.js",
    "test:integration": "bin/kill-process && mocha test/integration/setup.fixture.js test/integration/{specs,specs/**}/*.spec.js test/integration/teardown.fixture.js --opts test/integration/mocha.opts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JobtechSwe/gig-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JobtechSwe/gig-api/issues"
  },
  "homepage": "https://github.com/JobtechSwe/gig-api#readme",
  "nodemonConfig": {
    "ignore": [
      "migrations/*",
      "test/integration/migrations/*",
      "bin/*"
    ]
  },
  "dependencies": {
    "camelcase-keys": "4.2.0",
    "cors": "2.8.4",
    "elasticsearch": "15.1.1",
    "graphql-request": "1.6.0",
    "moment": "^2.22.2",
    "nconf": "0.10.0",
    "node-pg-migrate": "3.3.0",
    "path": "0.12.7",
    "require-dir": "1.0.0",
    "restify": "7.2.0",
    "simple-pg": "1.0.0",
    "snakecase-keys": "1.1.1",
    "striptags": "3.1.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-uuid": "^1.0.6",
    "chalk": "^2.4.1",
    "dot-object": "^1.7.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "proxyquire": "^2.0.1",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "sinon": "^5.0.10",
    "sinon-chai": "^3.1.0",
    "uuid": "^3.2.1"
  }
}
